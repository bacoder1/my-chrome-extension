{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0.0",
  "description": "A Chrome extension built with Vite and React",
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.tsx-loader-D5iU8IhI.js"
      ],
      "matches": [
        "https://*.index-education.net/pronote/eleve.html"
      ],
      "css": [
        "assets/content-7YDP4q22.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "fonts/fixel/*.ttf"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://*.index-education.net/*"
      ],
      "resources": [
        "assets/icons-BAtk9n0V.js",
        "assets/content.tsx-Cyc5LLkj.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "permissions": [
    "storage"
  ]
}
