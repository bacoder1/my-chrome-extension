{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0.0",
  "description": "A Chrome extension built with Vite and React",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.tsx-loader-CAl4tCRd.js"
      ],
      "matches": [
        "https://*.index-education.net/pronote/eleve.html"
      ],
      "css": [
        "assets/content-BxT3b9-8.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "fonts/fixel/*.ttf"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://*.index-education.net/*"
      ],
      "resources": [
        "assets/icons-CL6IDfZM.js",
        "assets/content.tsx-BJm88U-5.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "permissions": [
    "storage"
  ]
}
